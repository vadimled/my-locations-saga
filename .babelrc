{
    "presets": [
        ["env", {
            "targets": {
                "browsers": [
                    "> 1%",
                    "last 2 versions"
                ]
            }
        }],
        "stage-2",
        "react"
    ],
    "plugins": [
        "syntax-dynamic-import",
        ["transform-object-rest-spread", { "useBuiltIns": true }],
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ]
}